@model UserInterface.Models.InvoiceAgeingSummary

<div class="col-md-6 ">
    <div class="box box-widget widget-user">
        <!-- Add the bg color to the header using any of the bg-* classes -->
        <div class="widget-user-header bg-yellow-active" style="height:80px">
            <h5 class="widget-user-username" style="font-size:20px">Customer Invoices Due </h5>
            <h5 class="widget-user-desc">Number of invoices on Due  </h5>
        </div>

        <div class="box-footer">
            <div class="row">
                <div class="col-sm-4 border-right border-bottom">
                    <div class="description-block dashLink" onclick="OpenCustomerInvoiceRegister('?id=7days&isDashboard=1')">
                        <h5 class="description-header">@Model.CustInvAgeSummary.days7</h5>
                        <span class="description-text">Due in 7 Days</span>
                    </div>
                    <!-- /.description-block -->
                </div>
                <!-- /.col -->
                <div class="col-sm-4 border-right border-bottom">
                    <div class="description-block dashLink" onclick="OpenCustomerInvoiceRegister('?id=Today&isDashboard=1')">
                        <h5 class="description-header">@Model.CustInvAgeSummary.Todays</h5>
                        <span class="description-text">Today</span>
                    </div>
                    <!-- /.description-block -->
                </div>
                <!-- /.col -->
                <div class="col-sm-4 border-bottom">
                    <div class="description-block dashLink" onclick="OpenCustomerInvoiceRegister('?id=1To30&isDashboard=1')">
                        <h5 class="description-header">@Model.CustInvAgeSummary.Count1To30</h5>
                        <span class="description-text">1-30 Days</span>
                    </div>
                    <!-- /.description-block -->
                </div>


            </div>
            <div class="row">
                <div class="col-sm-4 border-right">
                    <div class="description-block dashLink" onclick="OpenCustomerInvoiceRegister('?id=31To60&isDashboard=1')">
