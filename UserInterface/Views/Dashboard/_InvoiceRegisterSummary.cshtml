                        <span class="description-text">91 Above</span>
                    </div>
                    <!-- /.description-block -->
                </div>
            </div>
            <!-- /.row -->
            <div class="clearfix">

                <a href="../Report/CustomerPaymentExpeditingDetails/" class="label bg-yellow">View Details</a>


            </div>
        </div>
    </div>
</div>

